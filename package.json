{
  "name": "musicplayer",
  "version": "1.0.0",
  "description": "Music player with extra customizations",
  "homepage": "./",
  "main": "./public/main.js",
  "scripts": {
    "test": "test",
    "start": "cd react-app && npm run start",
    "all": "concurrently -k \" npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-win": "electron-packager . MusicPlayer --platform=win32 --arch=x64 --out=release-builds",
    "react-build": "react-scripts build",
    "electron-build": "electron-builder",
    "build": "npm react-build && npm electron-build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "electron": "^26.0.0",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "glob": "^10.3.3",
    "music-metadata": "^8.1.4",
    "react-scripts": "^5.0.1"
  }
}
