{
  "name": "musicplayer",
  "version": "1.0.0",
  "description": "Music player with extra customizations",
  "homepage": "./",
  "main": "./public/electron.js",
  "scripts": {
    "react-start": "react-scripts start",
    "all": "concurrently -k \" npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-win": "electron-packager . MusicPlayer --platform=win32 --arch=x64 --out=release-builds",
    "react-build": "react-scripts build",
    "electron-build": "electron-builder",
    "build": "npm react-build && npm electron-build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "electron": "^26.0.0",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "ffmpeg-static": "^5.2.0",
    "glob": "^10.3.3",
    "music-metadata": "^8.1.4",
    "node-fetch": "^3.3.2",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "spotify-web-api-node": "^5.0.2",
    "tone": "^14.7.77",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
